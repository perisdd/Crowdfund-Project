@page
@using Crowdfund.Models;
@model Crowdfund.Pages.Creators.IndexModel
@{
    ViewData["Title"] = "Creators";
}
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="page-content">
                <div class="main-banner">
                    <div class="row">
                        <div class="col-lg-7">
                            <div class="header-text">
                                <h6></h6>
                                <h4>you are at <em>Crators/Index</em> here</h4>
<h1>Creators</h1>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Creator creator in Model.Creators)
        {
                <tr>
                    <td>@creator.FirstName</td>
                    <td>@creator.LastName</td>
                </tr>
        }
    </tbody>
</table>
